[*.cs]

# CS8620: 由于引用类型的可为 null 性差异，实参不能用于形参。
dotnet_diagnostic.CS8620.severity = silent

# CS8604: 引用类型参数可能为 null。
dotnet_diagnostic.CS8604.severity = suggestion

# CS8618: 在退出构造函数时，不可为 null 的字段必须包含非 null 值。请考虑添加 "required" 修饰符或声明为可为 null。
dotnet_diagnostic.CS8618.severity = suggestion

# IDE0008: 使用显式类型
dotnet_diagnostic.IDE0008.severity = suggestion
